{
  "env": {
      "browser": true,
      "es2021": true
  },
  "extends": [
      "airbnb-base",
      "eslint-config-prettier"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
  },
  "plugins": [
      "@typescript-eslint",
      "eslint-plugin-prettier"
  ],
  "rules": {
      "linebreak-style": 0,
      "class-methods-use-this": 0,
      "prettier/prettier": "error"
  },
  "settings": {
      "import/resolver": {
          "node": {
              "extensions": [".js", ".ts"]
          }
      }
  }
}